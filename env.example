# WS Deal Dash - Environment Variables Example
# Copy this file to .env.local for local development

# App Configuration
VITE_APP_NAME=WS Deal Dash
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Luxury CRM & Deal Management System

# Build Configuration
VITE_NODE_ENV=production

# Mock Data Mode (set to true for demo mode)
VITE_USE_MOCK_DATA=true

# If you want to use real Supabase (set VITE_USE_MOCK_DATA=false)
# VITE_SUPABASE_URL=your_supabase_url_here
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Analytics (optional)
# VITE_GA_TRACKING_ID=your_google_analytics_id

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false
